package fr.zak.cubesedge;

import net.minecraft.entity.player.EntityPlayer;
import fr.zak.cubesedge.entity.EntityPlayerCustom;

public abstract class IMovementClient {

	public abstract void controlClient(MovementFields playerCustom, EntityPlayer player);
	
	public abstract void renderTick(MovementFields playerCustom);

	public abstract String getName();

}
